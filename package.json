{
    "name": "app-express-typeorm",
    "version": "0.0.1",
    "description": "Awesome project developed with TypeORM.",
    "typeOrmConfig": {
        "config": "src/config/enviroment/development.json"
    },
    "dependencies": {
        "apollo-server-express": "^1.3.6",
        "body-parser": "^1.18.1",
        "composable-middleware": "^0.3.0",
        "cookie-parser": "^1.3.5",
        "ejs": "^2.5.3",
        "errorhandler": "^1.4.2",
        "express": "^4.16.3",
        "express-graphql": "^0.6.12",
        "express-jwt": "^5.0.0",
        "express-session": "^1.11.3",
        "graphql": "^0.13.2",
        "graphql-s2s": "^0.16.5",
        "graphql-tools": "^3.0.2",
        "jsonwebtoken": "^7.0.0",
        "lodash": "^4.17.4",
        "lusca": "^1.4.1",
        "merge-graphql-schemas": "^1.5.2",
        "moment": "^2.21.0",
        "morgan": "^1.9.0",
        "nodemon": "^1.17.3",
        "passport": "~0.4.0",
        "passport-facebook": "^2.0.0",
        "passport-google-oauth2": "^0.1.6",
        "passport-local": "^1.0.0",
        "pg": "^7.3.0",
        "reflect-metadata": "^0.1.10",
        "serve-favicon": "^2.3.0",
        "typeorm": "^0.2.3",
        "winston": "^3.0.0-rc3"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "^7.2.6",
        "@types/node": "^8.0.29",
        "ts-node": "3.3.0",
        "typescript": "2.5.2"
    },
    "scripts": {
        "start": "ts-node src/index.ts",
        "dev": "set NODE_ENV=development&& ts-node ./node_modules/nodemon/bin/nodemon.js",
        "prod": "set NODE_ENV=production&& ts-node ./node_modules/nodemon/bin/nodemon.js",
        "typeorm:dev": "typeorm --config ./development.json",
        "schema:sync:dev": "ts-node ./node_modules/typeorm/cli.js --config ./development.json schema:sync",
        "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
        "migrations:run": "ts-node ./node_modules/typeorm/cli.js migrations:run"
    }
}
